{
  "dashboard": {
    "title": "Phone Book - JVM Performance",
    "tags": ["phone-book", "jvm", "performance", "spring-boot"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Heap Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"spring-boot\",area=\"heap\"}",
            "legendFormat": "Used",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_committed_bytes{job=\"spring-boot\",area=\"heap\"}",
            "legendFormat": "Committed",
            "refId": "B"
          },
          {
            "expr": "jvm_memory_max_bytes{job=\"spring-boot\",area=\"heap\"}",
            "legendFormat": "Max",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Non-Heap Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"spring-boot\",area=\"nonheap\"}",
            "legendFormat": "Used",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_committed_bytes{job=\"spring-boot\",area=\"nonheap\"}",
            "legendFormat": "Committed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "GC Pause Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_sum{job=\"spring-boot\"}[5m])",
            "legendFormat": "{{action}} - {{cause}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "GC Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "GC Count",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_count{job=\"spring-boot\"}[5m])",
            "legendFormat": "{{action}} - {{cause}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "GC/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Thread Count",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "jvm_threads_live{job=\"spring-boot\"}",
            "legendFormat": "Live",
            "refId": "A"
          },
          {
            "expr": "jvm_threads_daemon{job=\"spring-boot\"}",
            "legendFormat": "Daemon",
            "refId": "B"
          },
          {
            "expr": "jvm_threads_peak{job=\"spring-boot\"}",
            "legendFormat": "Peak",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Threads"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Thread States",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "jvm_threads_states_threads{job=\"spring-boot\"}",
            "legendFormat": "{{state}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Threads"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Classes Loaded",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "jvm_classes_loaded{job=\"spring-boot\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "title": "CPU Usage",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "process_cpu_usage{job=\"spring-boot\"} * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "System CPU Usage",
        "type": "gauge",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "system_cpu_usage{job=\"spring-boot\"} * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "System Load Average",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "system_load_average_1m{job=\"spring-boot\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ]
  },
  "overwrite": true
}
